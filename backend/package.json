{
  "name": "backend",
  "main": "src/index.ts",
  "scripts": {
    "dev": "nodemon",
    "prod": "node ./build/src/index.js",
    "build": "tsc",
    "test": "jest",
    "coverage": "jest --coverage",
    "coverage:open": "node scripts/run-coverage-and-open-report.mjs",
    "test:unit": "jest --testPathPattern=/__tests__/unit",
    "test:integration": "jest --testPathPattern=/__tests__/integration",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js ",
    "migration:generate": "npm run typeorm -- migration:generate -d src/database/dataSource.ts",
    "migration:run": "npm run build && npm run typeorm -- migration:run -d src/database/dataSource.ts",
    "migration:run:no-build": "npm run typeorm -- migration:run -d src/database/dataSource.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/database/dataSource.ts",
    "migration:create": "npm run typeorm -- migration:create"
  },
  "dependencies": {
    "@bcgov/citz-imb-endpoint-builder": "1.0.0-beta2",
    "@bcgov/citz-imb-express-utilities": "1.0.0-beta3",
    "@bcgov/citz-imb-sso-css-api": "1.0.0-rc",
    "@bcgov/citz-imb-sso-express": "1.0.0-rc",
    "cors": "2.8.5",
    "express": "4.19.2",
    "express-rate-limit": "7.3.1",
    "pg": "8.12.0",
    "typeorm": "0.3.20",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/node": "20.14.5",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "7.13.1",
    "@typescript-eslint/parser": "7.13.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "jest": "29.7.0",
    "nodemon": "3.1.3",
    "prettier": "3.3.2",
    "supertest": "7.0.0",
    "ts-jest": "29.1.5",
    "ts-node": "10.9.2",
    "tsx": "4.15.6",
    "typescript": "5.4.5"
  }
}
