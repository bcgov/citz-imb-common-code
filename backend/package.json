{
  "name": "backend",
  "main": "src/index.ts",
  "scripts": {
    "dev": "nodemon",
    "prod": "node ./build/index.js",
    "build": "tsc",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js ",
    "migration:generate": "npm run typeorm -- migration:generate -d src/dataSource.ts",
    "migration:run": "npm run build && npm run typeorm -- migration:run -d src/dataSource.ts",
    "migration:run:no-build": "npm run typeorm -- migration:run -d src/dataSource.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/dataSource.ts",
    "migration:create": "npm run typeorm -- migration:create"
  },
  "dependencies": {
    "@bcgov/citz-imb-kc-express": "0.0.1",
    "cors": "2.8.5",
    "express": "4.18.3",
    "express-rate-limit": "7.2.0",
    "pg": "8.11.3",
    "reflect-metadata": "0.2.1",
    "rss-parser": "3.13.0",
    "typeorm": "0.3.20",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/node": "20.11.24",
    "@types/pg": "8.11.2",
    "@typescript-eslint/eslint-plugin": "7.1.1",
    "@typescript-eslint/parser": "7.1.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "nodemon": "3.1.0",
    "prettier": "3.2.5",
    "ts-node": "10.9.2",
    "tsx": "4.7.1",
    "typescript": "5.3.3"
  }
}
